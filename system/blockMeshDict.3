FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      blockMeshDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 1;

xmin 0;
xmax1 0.01;
ymin 0;
ymax 0.4;//0.5;
zmin 0;
zmax 0.001;//0.001;

xmax2 0.012;



vertices
(
    ($xmin  $ymin  $zmin) //0
    ($xmax1  $ymin  $zmin) //1
    ($xmax1  $ymax  $zmin) //2
    ($xmin  $ymax  $zmin) //3
    ($xmin  $ymin  $zmax) //4
    ($xmax1  $ymin  $zmax) //5
    ($xmax1  $ymax  $zmax) //6
    ($xmin  $ymax  $zmax) //7

	($xmax2 $ymin $zmin) //8
	($xmax2	$ymax $zmin) //9
	($xmax2 $ymin $zmax) //10
	($xmax2 $ymax $zmax) //11




);

	
blocks
(
    hex (0 1 2 3 4 5 6 7) (24 3 1) simpleGrading (1 1 1)
	
	hex(1 8 9 2 ) (24 3 1) simpleGrading (1 1 1)
);

edges
(
);

boundary
(
	top
	{
		type wall;
			
		faces
		(
			(3 2 7 6)
		);
	}

	bot
	{
		type wall;

		faces
		(
			(0 1 5 4)
		);
	}

	symWall
	{
		type symmetryPlane;

		faces
		(
			(0 3 7 4)
		);
	}

	front
	{
		type empty;

		faces
		(
			(4 5 6 7)
		);
	}

	back
	{
		type empty;
		
		faces
		(
			(0 3 2 1)
		);
	}

	right
	{
		type wall;

		faces
		(
			(1 2 6 5)
		);
	}
);

mergePatchPairs
(
);

